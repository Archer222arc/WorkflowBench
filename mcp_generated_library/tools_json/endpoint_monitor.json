{
  "name": "endpoint_monitor",
  "description": "Performs monitor operations on endpoint data",
  "parameters": {
    "type": "object",
    "properties": {
      "endpoint": {
        "type": "string",
        "description": "The url to process",
        "pattern": "^https?://.*"
      }
    },
    "required": [
      "endpoint"
    ]
  },
  "returns": {
    "response": {
      "type": "object",
      "description": "The resulting response"
    }
  },
  "errors": [
    {
      "code": "TIMEOUT",
      "description": "Operation timed out"
    },
    {
      "code": "AUTH_FAILED",
      "description": "Authentication failed"
    },
    {
      "code": "CONNECTION_ERROR",
      "description": "Failed to establish connection"
    }
  ],
  "metadata": {
    "category": "api_integration",
    "avg_execution_time": 1.8,
    "success_rate": 0.92,
    "rate_limit": 100,
    "requires_auth": true
  }
}